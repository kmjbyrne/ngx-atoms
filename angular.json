{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@kbpc/components": {
      "projectType": "library",
      "root": "projects/kbpc/components",
      "sourceRoot": "projects/kbpc/components/src",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/kbpc/components/tsconfig.lib.json",
            "project": "projects/kbpc/components/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kbpc/components/src/test.ts",
            "tsConfig": "projects/kbpc/components/tsconfig.spec.json",
            "karmaConfig": "projects/kbpc/components/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/kbpc/components/tsconfig.lib.json",
              "projects/kbpc/components/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@kbpc/atoms": {
      "projectType": "library",
      "root": "projects/kbpc/atoms",
      "sourceRoot": "projects/kbpc/atoms/src",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/kbpc/atoms/tsconfig.lib.json",
            "project": "projects/kbpc/atoms/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kbpc/atoms/src/test.ts",
            "tsConfig": "projects/kbpc/atoms/tsconfig.spec.json",
            "karmaConfig": "projects/kbpc/atoms/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/kbpc/atoms/tsconfig.lib.json",
              "projects/kbpc/atoms/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@kbpc/http": {
      "projectType": "library",
      "root": "projects/kbpc/http",
      "sourceRoot": "projects/kbpc/http/src",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/kbpc/http/tsconfig.lib.json",
            "project": "projects/kbpc/http/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/kbpc/http/src/test.ts",
            "tsConfig": "projects/kbpc/http/tsconfig.spec.json",
            "karmaConfig": "projects/kbpc/http/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/kbpc/http/tsconfig.lib.json",
              "projects/kbpc/http/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "@kbpc/components"
}